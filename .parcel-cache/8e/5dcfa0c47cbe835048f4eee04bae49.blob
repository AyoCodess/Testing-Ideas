const button = document.querySelector(".test");
const but = document.querySelector(".btncon");
const userInput = document.querySelector(".input");
const lists = document.querySelector(".main");

const greaterThan0 = function () {
    return userInput.value.length;
};

const createElementList = function () {
    let title = document.createElement("h3");
    title.appendChild(document.createTextNode(userInput.value));
    lists.appendChild(title);
    userInput.value = "";
};

const clickEvent = function () {
    if (greaterThan0() > 0) {
        createElementList();
    }
};

const enterPressed = function (event) {
    if (greaterThan0() > 0 && event.key === "Enter") {
        console.log(event);
        createElementList();
    }
};

button.addEventListener("click", clickEvent);
but.addEventListener("keypress", enterPressed);

const listItemDone = function () {
    title.addEventListener("click", function() {
        title.
    } )
