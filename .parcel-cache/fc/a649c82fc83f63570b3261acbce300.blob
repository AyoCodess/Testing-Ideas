const Calculator = function () {
    this.methods = {
        "-": (a, b) => a - b,
        "+": (a, b) => a + b,
        "/": (a, b) => a / b,
        "*": (a, b) => a * b,
    };

    this.calculate = function (str) {
        let split = str.split(" ");
        let a = split[0];
        let op = split[1];
        let b = split[2];

        console.log(a);
        console.log(op);
        console.log(b);

        if (!this.methods[op] || isNaN(a) || isNaN(b)) {
            return alert(`please try again`);
        }

        return this.methods[op](a, b);
    };
};

let calc = new Calculator();

alert(calc.calculate("h + g")); // 10
